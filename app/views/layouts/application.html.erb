<!DOCTYPE html>
<html>
<head>
  <title><%= page_title %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="container">
    <div id="header">
    <%= render 'common/header' %>
    </div>
    
    <div id="content">
    <%= yield %>
    </div>
    
    <div id="footer">
    <%= render 'common/footer' %>
    </div>
  </div>
</body>
</html>
